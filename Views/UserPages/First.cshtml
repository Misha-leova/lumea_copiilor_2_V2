@{
    ViewData["Title"] = "Acasă";
    Layout = "~/Views/Shared/_LayoutUser.cshtml";

}

<!-- Hero Banner -->
<div class="hero-banner rounded mb-4">
    <div class="container py-5">
        <div class="row align-items-center">
            <div class="col-md-6">
                <h1 class="display-4 fw-bold text-white">Bine ai venit în Lumea Jucăriilor!</h1>
                <p class="lead text-white">Descoperă universul magic al jucăriilor unde fiecare zi este o nouă aventură!</p>
                <div class="mt-4">
                    <a  class="btn btn-toy btn-lg me-2" asp-controller="UserPages" asp-action="Market">Explorează jucăriile</a>
                    <a href="#promotii" class="btn btn-light btn-lg">Vezi promoțiile</a>
                </div>
            </div>
            <div class="col-md-6 text-center">
                <img src="https://etimg.etb2bimg.com/thumb/msid-94749754,imgsize-148476,width-1200,height=765,overlay-etretail/toys-kids-and-baby/toys/toy-industry-can-generate-30000-jobs-in-tn-with-govt-support.jpg" alt="Copii fericiți cu jucării" class="img-fluid rounded animated-float" />
            </div>
        </div>
    </div>
</div>

<!-- Categorii Populare -->
<section id="categorii" class="mb-5">
    <div class="section-header text-center mb-4">
        <h2 class="display-5 fw-bold">Categorii populare</h2>
        <p class="text-muted">Descoperă cele mai îndrăgite jucării pentru copilul tău</p>
    </div>

    <div class="row">
        <div class="col-md-3 mb-4">
            <div class="category-card h-100">
                <img src="data:image/jpeg;base64," alt="Jucării educative" class="card-img-top" />
                <div class="card-body text-center">
                    <h4 class="card-title">Jucării educative</h4>
                    <p class="card-text">Învățare prin joacă și descoperire</p>
                    <a href="#" class="btn btn-toy">Descoperă</a>
                </div>
            </div>
        </div>
        <div class="col-md-3 mb-4">
            <div class="category-card h-100">
                <img src="https://m.media-amazon.com/images/I/51lxFyn9iYL._AC_UF1000,1000_QL80_.jpg" alt="Păpuși și accesorii" class="card-img-top" />
                <div class="card-body text-center">
                    <h4 class="card-title">Păpuși și accesorii</h4>
                    <p class="card-text">Prieteni de joacă pentru cei mici</p>
                    <a href="#" class="btn btn-toy">Descoperă</a>
                </div>
            </div>
        </div>
        <div class="col-md-3 mb-4">
            <div class="category-card h-100">
                <img src="https://i.ebayimg.com/images/g/VEcAAOSwVfdjpJZJ/s-l1200.jpg" alt="Mașinuțe și vehicule" class="card-img-top" />
                <div class="card-body text-center">
                    <h4 class="card-title">Mașinuțe și vehicule</h4>
                    <p class="card-text">Aventuri de neuitat pe patru roți</p>
                    <a href="#" class="btn btn-toy">Descoperă</a>
                </div>
            </div>
        </div>
        <div class="col-md-3 mb-4">
            <div class="category-card h-100">
                <img src="https://www.thetoychronicle.com/wp-content/uploads/2024/10/Kong-Andri-x-Museum-of-Toys-Presents-PORTER-BOY-Urban-Sneaker-Society-Exclusive.jpg" alt="Jocuri de societate" class="card-img-top" />
                <div class="card-body text-center">
                    <h4 class="card-title">Jocuri de societate</h4>
                    <p class="card-text">Distracție pentru toată familia</p>
                    <a href="#" class="btn btn-toy">Descoperă</a>
                </div>
            </div>
        </div>
    </div>

    <div class="text-center mt-3">
        <a href="#" class="btn btn-lg btn-outline-primary see-all-btn">Vezi toate categoriile <i class="fas fa-arrow-right ms-2"></i></a>
    </div>
</section>

<!-- Banner promoții -->
<section id="promotii" class="mb-5">
    <div class="promo-banner rounded py-4">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-md-7">
                    <h2 class="display-6 text-white fw-bold">Super reduceri de primăvară!</h2>
                    <p class="lead text-white">Până la 50% reducere la jucăriile preferate în perioada 10-20 martie</p>
                    <a href="#" class="btn btn-light btn-lg mt-3">Profită acum</a>
                </div>
                <div class="col-md-5 text-center">
                    <div class="discount-badge">
                        <span class="display-4 fw-bold">-50%</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- Avantaje -->
<section class="features-section mb-5 py-4 rounded">
    <div class="container">
        <div class="row text-center">
            <div class="col-md-3 mb-4 mb-md-0">
                <div class="feature-item">
                    <div class="feature-icon mb-3">
                        <i class="fas fa-truck fa-3x"></i>
                    </div>
                    <h4>Livrare gratuită</h4>
                    <p>Pentru comenzi peste 200 lei</p>
                </div>
            </div>
            <div class="col-md-3 mb-4 mb-md-0">
                <div class="feature-item">
                    <div class="feature-icon mb-3">
                        <i class="fas fa-shield-alt fa-3x"></i>
                    </div>
                    <h4>Garanție de calitate</h4>
                    <p>Toate produsele sunt verificate</p>
                </div>
            </div>
            <div class="col-md-3 mb-4 mb-md-0">
                <div class="feature-item">
                    <div class="feature-icon mb-3">
                        <i class="fas fa-exchange-alt fa-3x"></i>
                    </div>
                    <h4>Returnare ușoară</h4>
                    <p>30 de zile pentru returnare</p>
                </div>
            </div>
            <div class="col-md-3">
                <div class="feature-item">
                    <div class="feature-icon mb-3">
                        <i class="fas fa-headset fa-3x"></i>
                    </div>
                    <h4>Suport 24/7</h4>
                    <p>Suntem mereu aici să te ajutăm</p>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- Testimoniale -->
<section class="testimonials-section mb-5">
    <div class="section-header text-center mb-4">
        <h2 class="display-5 fw-bold">Ce spun clienții noștri</h2>
        <p class="text-muted">Experiențe reale ale familiilor fericite</p>
    </div>

    <div class="row">
        <div class="col-md-4 mb-4">
            <div class="testimonial-card p-4 rounded h-100">
                <div class="testimonial-rating mb-3">
                    <i class="fas fa-star text-warning"></i>
                    <i class="fas fa-star text-warning"></i>
                    <i class="fas fa-star text-warning"></i>
                    <i class="fas fa-star text-warning"></i>
                    <i class="fas fa-star text-warning"></i>
                </div>
                <p class="testimonial-text">"Jucăriile educative sunt extraordinare! Fiica mea a învățat literele și numerele jucându-se. Livrarea a fost rapidă și ambalajul foarte sigur."</p>
                <div class="testimonial-author d-flex align-items-center mt-3">
                    <img src="https://people.com/thmb/gzHtG_UnZBsUuHVJx9xjB5yAfIQ=/4000x0/filters:no_upscale():max_bytes(150000):strip_icc():focal(399x0:401x2)/people-headshot-nick-maslow-f21ef38676504bc89a091ec9a5c95e4b.jpg" />
                    <div>
                        <h5 class="mb-0">Ana Popescu</h5>
                        <small class="text-muted">Mamă a 2 copii</small>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-4 mb-4">
            <div class="testimonial-card p-4 rounded h-100">
                <div class="testimonial-rating mb-3">
                    <i class="fas fa-star text-warning"></i>
                    <i class="fas fa-star text-warning"></i>
                    <i class="fas fa-star text-warning"></i>
                    <i class="fas fa-star text-warning"></i>
                    <i class="fas fa-star text-warning"></i>
                </div>
                <p class="testimonial-text">"Sunt foarte mulțumit de calitatea jucăriilor. Fiul meu adoră seturile LEGO, iar prețurile sunt mult mai bune decât în alte magazine. Recomand cu încredere!"</p>
                <div class="testimonial-author d-flex align-items-center mt-3">
                    <img src="https://www.allprodad.com/wp-content/uploads/2021/03/05-12-21-happy-people.jpg" alt="Client" class="rounded-circle me-3" />
                    <div>
                        <h5 class="mb-0">Mihai Ionescu</h5>
                        <small class="text-muted">Tată și profesor</small>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-4 mb-4">
            <div class="testimonial-card p-4 rounded h-100">
                <div class="testimonial-rating mb-3">
                    <i class="fas fa-star text-warning"></i>
                    <i class="fas fa-star text-warning"></i>
                    <i class="fas fa-star text-warning"></i>
                    <i class="fas fa-star text-warning"></i>
                    <i class="fas fa-star-half-alt text-warning"></i>
                </div>
                <p class="testimonial-text">"Serviciul clienți este excepțional! Am avut o mică problemă cu o comandă și a fost rezolvată în aceeași zi. Jocurile de societate sunt perfecte pentru serile în familie."</p>
                <div class="testimonial-author d-flex align-items-center mt-3">
                    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcShkv_Hr6sv5PmBN_BVwnrDxDtBBEOv6N9ID-JBtBcxcGXPwaiZk8pPuHJ8o6moKejtlJ8&usqp=CAU" alt="Client" class="rounded-circle me-3" />
                    <div>
                        <h5 class="mb-0">Elena Dumitrescu</h5>
                        <small class="text-muted">Clientă fidelă</small>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- Newsletter -->
<section class="newsletter-section rounded mb-5 p-4">
    <div class="row align-items-center">
        <div class="col-md-6">
            <h3 class="text-white mb-2">Abonează-te la newsletter</h3>
            <p class="text-white mb-0">Primește noutăți despre promoții și jucării noi</p>
        </div>
        <div class="col-md-6">
            <div class="input-group">
                <input type="email" class="form-control" placeholder="Adresa ta de email" aria-label="Email" aria-describedby="button-addon2">
                <button class="btn btn-light" type="button" id="button-addon2">Abonare</button>
            </div>
        </div>
    </div>
</section>

<style>
    /* Hero Banner */
    .hero-banner {
        background: linear-gradient(135deg, var(--primary-color), var(--secondary-color));
        overflow: hidden;
        position: relative;
    }

    .animated-float {
        animation: float 6s ease-in-out infinite;
        box-shadow: 0 15px 30px rgba(0,0,0,0.15);
    }

    @@keyframes float {
        0% {
            transform: translateY(0px);
        }

        50% {
            transform: translateY(-20px);
        }

        100% {
            transform: translateY(0px);
        }
    }

    /* Categorii */
    .category-card {
        border: none;
        background-color: white;
        transition: all 0.3s;
    }

        .category-card:hover {
            box-shadow: 0 10px 25px rgba(0,0,0,0.1);
        }

    .see-all-btn {
        border: 2px solid var(--primary-color);
        color: var(--primary-color);
        font-weight: bold;
        padding: 10px 30px;
    }

        .see-all-btn:hover {
            background-color: var(--primary-color);
            color: white;
        }

    /* Promo Banner */
    .promo-banner {
        background: linear-gradient(135deg, #FF416C, #FF4B2B);
        overflow: hidden;
    }

    .discount-badge {
        background-color: rgba(255, 255, 255, 0.2);
        border-radius: 50%;
        width: 150px;
        height: 150px;
        display: flex;
        align-items: center;
        justify-content: center;
        color: white;
        margin: 0 auto;
        animation: pulse 2s infinite;
    }

    @@keyframes pulse {
        0% {
            transform: scale(1);
            box-shadow: 0 0 0 0 rgba(255, 255, 255, 0.5);
        }

        70% {
            transform: scale(1.05);
            box-shadow: 0 0 0 20px rgba(255, 255, 255, 0);
        }

        100% {
            transform: scale(1);
        }
    }

    /* Produse */
    .product-card {
        border-radius: 15px;
        background-color: white;
        box-shadow: 0 5px 15px rgba(0,0,0,0.08);
        transition: all 0.3s;
        overflow: hidden;
        position: relative;
    }

        .product-card:hover {
            transform: translateY(-10px);
            box-shadow: 0 15px 25px rgba(0,0,0,0.1);
        }

    .badge-new {
        position: absolute;
        top: 10px;
        right: 10px;
        background-color: var(--accent-color);
        color: var(--dark-color);
        font-weight: bold;
        padding: 5px 10px;
        border-radius: 20px;
        font-size: 0.8rem;
    }

    .product-title {
        height: 48px;
        overflow: hidden;
        margin-bottom: 10px;
    }

    .price-new {
        color: var(--primary-color);
        font-weight: bold;
        font-size: 1.2rem;
    }

    /* Vârstă și branduri */
    .age-section, .brands-section {
        background-color: white;
        box-shadow: 0 5px 15px rgba(0,0,0,0.05);
        height: 100%;
    }

    .age-category {
        padding: 10px;
        border-radius: 10px;
        transition: all 0.3s;
    }

        .age-category:hover {
            background-color: rgba(255, 107, 107, 0.1);
        }

    .age-icon {
        color: var(--primary-color);
    }

    .brand-logo {
        border: 1px solid #eee;
        border-radius: 10px;
        transition: all 0.3s;
    }

        .brand-logo:hover {
            box-shadow: 0 5px 15px rgba(0,0,0,0.1);
        }

    /* Features */
    .features-section {
        background-color: white;
        box-shadow: 0 5px 15px rgba(0,0,0,0.05);
    }

    .feature-icon {
        color: var(--primary-color);
    }

    .feature-item {
        padding: 20px;
        transition: all 0.3s;
    }

        .feature-item:hover {
            transform: translateY(-5px);
        }

    /* Testimoniale */
    .testimonial-card {
        background-color: white;
        box-shadow: 0 5px 15px rgba(0,0,0,0.05);
        transition: all 0.3s;
    }

        .testimonial-card:hover {
            box-shadow: 0 10px 25px rgba(0,0,0,0.1);
        }

    .testimonial-text {
        font-style: italic;
        line-height: 1.6;
    }

    /* Newsletter */
    .newsletter-section {
        background: linear-gradient(135deg, var(--secondary-color), var(--primary-color));
    }
</style>